    <script type="text/javascript">
      
        function ExibirImagemCancelada() {
            var objImg = document.getElementById('imgInvalido1');
            if ($('#hdfNotaCancelada').length) {
                AcertarDivErro(objImg);
            }
        }

        function ExibirImagemDenegada() {
            var objImg = document.getElementById('imgInvalido2');
            if ($('#hdfNotaDenegada').length) {
                AcertarDivErro(objImg);
            }
        }

        function AjustarExibicaoEPEC()
        {
            if ($('#hdfSomenteEPEC').length) {
                $('.ui-page').css({ "minHeight": "500px" });
            }
        }

        function AcertarDivErro(objImg) {
            $('#erro').css({ "display": "block" });
            $('#erro').css({ "maxWidth": "510px" });
            $('#erro').css({ "maxHeight": "200px" });
            objImg.style.display = 'block';
        }
    </script>
